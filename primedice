#include <bits/stdc++.h>
using namespace std;
int main() {
	int t;
	cin>>t;
	while(t--){
	    int a, b, s1, s2;
	    cin>>a>>b;
	    s1=a+b;
	    s2=s1/2;
	    string flag="alice";
	    
	    for(int i=2; i<=s2; i++){
	        if(s1%i==0){
	            flag="bob";
	            break;
	        }
	    }
	    cout<<flag<<endl;
	}
	return 0;
}
