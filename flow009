#include <bits/stdc++.h>
using namespace std;
int main() {
    int t, q, p;
    double total;
    cin>>t;
    while(t>0) {
        cin>>q>>p;
        if (q>1000) {
            total= (q*p) - (0.1*q*p);
            cout<<fixed<<setprecision(6)<<total<<endl;
        }
        else {
        total=q*p;
        cout<<fixed<<setprecision(6)<<total<<endl;
        }
        t--;
    }
	return 0;
}
