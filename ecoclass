#include <iostream>
using namespace std;

int main() {
	int n, i, t;
	cin>>t;
	while (t>0) {
	    int count=0;
	    cin>>n;
	    int s[n], d[n];
	    
	    for (i=0; i<n; i++){
	        cin>>s[i];
	    }
	    
	    for (i=0; i<n; i++){
	        cin>>d[i];
	    }
	    
	    for (i=0; i<n; i++){
	        if (s[i]== d[i]) count++;
	    }
	    
	    cout<<count<<endl;
	    
	    t--;
	}
	return 0;
}
