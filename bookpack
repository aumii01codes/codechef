#include <bits/stdc++.h>
using namespace std;
int main() {
	int t;
	cin>>t;
	while(t--){
	    float x, y, z;
	    cin>>x>>y>>z;
	    if(y>z){
	        int box= ceil(y/z);
	        int total= box*x;
	        cout<<total<<endl;
	    }
	    else{
	        int num= (int) x;
	        cout<<num<<endl;
	    }
	}
	return 0;
}
