import java.util.*;
import java.lang.*;
import java.io.*;
class Codechef {
	public static void main (String[] args) throws java.lang.Exception {
		Scanner read=new Scanner(System.in);
		int x=read.nextInt();
		read.close();
		if(x<100) System.out.println("yes");
		else System.out.println("no");
	}
}
