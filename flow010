#include <iostream>

int main() {
    int t;
    std::cin >> t;

    char id;
    for (int i = 0; i < t; i++) {
        std::cin >> id;
        switch (id) {
            case 'B':
            case 'b':
                std::cout << "BattleShip" << std::endl;
                break;
            case 'C':
            case 'c':
                std::cout << "Cruiser" << std::endl;
                break;
            case 'D':
            case 'd':
                std::cout << "Destroyer" << std::endl;
                break;
            case 'F':
            case 'f':
                std::cout << "Frigate" << std::endl;
                break;
            default:
                std::cout << "Invalid ID" << std::endl;
                break;
        }
    }
    return 0;
}
