#include <iostream>
using namespace std;

int main() {
    int t, d, l, r;
    cin>>t;
    
    while (t>0){
        cin>>d>>l>>r;
        
        if (d<l) cout<<"Too Early"<<endl;
        else if (d>r) cout<<"Too Late"<<endl;
        else cout<<"Take second dose now"<<endl;
        
        t--;
    }
	return 0;
}
