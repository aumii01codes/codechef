#include <iostream>
using namespace std;
int main() {
	int t, a, b, c;
	cin>>t;
	while(t>0) {
	   cin>>a>>b>>c;
	   int left=0, right=0;
	   if ((a==0) || (b==0) || (c==0)) left++;
	   if ((a==1) || (b==1) || (c==1)) right++;
	    
	   if ((left>0) && (right>0)) cout<<"1"<<endl;
	   else cout<<"0"<<endl;
	 
	   t--;
	}
	return 0;
}
