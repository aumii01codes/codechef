#include <iostream>
using namespace std;
int main() {
    int t;
    cin>>t;
    while(t--){
        float a, b, x, y;
        cin>>a>>b>>x>>y;
        float chef=a/x;
        float chefina=b/y;
        if (chef<chefina) cout<<"chef"<<endl;
        else if  (chefina<chef) cout<<"chefina"<<endl;
        else cout<<"both"<<endl;
    }
	return 0;
}
