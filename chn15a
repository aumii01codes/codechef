#include <bits/stdc++.h>
using namespace std;
int main() {
	int t;
	cin>>t;
	while(t--){
	    int n, k, i;
	    cin>>n>>k;
	    int count=0;
	    for (i=0; i<n; i++){
	        int a;
	        cin>>a;
	        int dna= a+k;
	        if(dna%7==0) count++;
	    }
	    cout<<count<<endl;
	}
	return 0;
}
