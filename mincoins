#include <iostream>
using namespace std;
int main() {
	int t;
	cin>>t;
	while(t--){
	    int x, var10, var5;
	    int count=0;
	    cin>>x;
	    if (x%5==0){
	        var10=x/10;
	        x=x%10;
	        var5=x/5;
	        count= count + var10 + var5;
	        cout<<count<<endl;
	    }
	    else cout<<"-1"<<endl;
	}
	return 0;
}
