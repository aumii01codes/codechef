#include <iostream>
using namespace std;
int main() {
	int t;
	cin>>t;
	while(t--){
	    int a, b;
	    cin>>a>>b;
	    
	    if(a==b) cout<<"Yes"<<endl; //a==b
	    
	    
	    else if(a<b){ //a<b
	        while(a<b){ //while a is smaller than b, continue multiplying it by 2
	            a *= 2;
	        }
	        if(a==b) cout<<"Yes"<<endl; //once while is false, this condition will be checked
	        else cout<<"no"<<endl;
	    }
	    
	    
	    else{ //b<a
	        while(b<a){ //while b is smaller than a, continue multiplying it by 2
	            b *= 2;
	        }
	        if(a==b) cout<<"Yes"<<endl; //once while is false, this condition will be checked
	        else cout<<"no"<<endl;
	    }
	    
	}
	return 0;
}
