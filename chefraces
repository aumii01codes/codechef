#include <bits/stdc++.h>
using namespace std;
int main() {
	int t, a, b, x, y;
	cin>>t;
	while (t>0){
	    int count=0;
	    cin>>x>>y>>a>>b;
	    if((x!=a && x!=b) && (y!=a && y!=b)) cout<<"2"<<endl;
	    else if((x!=a && x!=b) || (y!=a && y!=b)) cout<<"1"<<endl;
	    else cout<<"0"<<endl;
	    t--;
	}
	return 0;
}
