#include <iostream>
using namespace std;
int main() {
	int t, a, b, c, d;
	cin>>t;
	while(t>0) {
	   cin>>a>>b>>c>>d;
	   
	   if (a>b) b=b+c;
	   else if (a<b) a=a+c;
	   else b=b+c;
	   
	   if (a>b) b=b+d;
	   else if (a<b) a=a+d;
	   else b=b+d;
	   
	   if (a>b) cout<<"N"<<endl;
	   else if (a<b) cout<<"S"<<endl;
	   else cout<<"N"<<endl;
	 
	   t--;
	}
	return 0;
}
