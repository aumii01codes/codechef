#include <iostream>
using namespace std;

int main() {
	int n;
	cin>>n;
	int lu=0, unlu=0, i;
	
	for (i=0; i<n; i++) {
	    int a;
	    cin>>a;
	    
	    if (a%2==0) {
	        lu++;
	    }
	    
	    else {
	        unlu++;
	    }
	}
	
	if (lu>unlu) cout<<"READY FOR BATTLE";
	else cout<<"NOT READY";
	
	return 0;
}
