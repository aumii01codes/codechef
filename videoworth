import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef {
  public static void main (String[] args) throws java.lang.Exception {
		Scanner read= new Scanner(System.in);
		int t= read.nextInt();
    for(int i=0; i<t; i++) {
		    int sec= read.nextInt();
		    int worth= sec * 24 * 1000;
		    System.out.println(worth);
		}
	}
}
