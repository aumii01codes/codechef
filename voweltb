#include <iostream>
using namespace std;

int main() {
	char c;
	cin>>c;
	switch(c)
	{
	    case 'A':
	    cout<<"Vowel";
	    break;
	    
	    case 'E':
	    cout<<"Vowel";
	    break;
	    
	    case 'I':
	    cout<<"Vowel";
	    break;
	    
	    case 'O':
	    cout<<"Vowel";
	    break;
	    
	    case 'U':
	    cout<<"Vowel";
	    break;
	    
	    default:
	    cout<<"Consonant";
	}
	return 0;
}
